<MWElements>
  <MWElement name="Cobra Eye Tracker">
    <!-- XPath 2.0 expression defining nodes of this type -->
    <match_signature>iodevice[@type="cobra_eye_tracker"]</match_signature>
    
    <isa>IODevice</isa>
    
    <description>
      A plugin to communicate with a Cobra Eyetracker running on the same system as the MW Server.
    </description>
    
    <!-- Payload -->
    <code>
      <iodevice  type="cobra_eye_tracker" tag="Cobra Eye Tracker" device_name="cobra1" executable_path="/Applications/EyeTracker.app/Contents/MacOS/EyeTracker">
        <iochannel type="cobra_eye_tracker" tag="Horizontal Gaze Angle" variable="gaze_h" capability="gaze_h"/>
        <iochannel type="cobra_eye_tracker" tag="Vertical Gaze Angle" variable="gaze_v" capability="gaze_v"/>
      </iodevice>
    </code>
  </MWElement>
</MWElements>
